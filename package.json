{
  "name": "music-app",
  "version": "1.0.0",
  "description": "This app will be used to organize music for all uses.",
  "main": "App.js",
  "scripts": {
    "build": "npm run clean && npm run build-next && npm run build-tsc && npm run copy-env",
    "build-next": "bash -c 'NODE_CONFIG_ENV=prod next build'",
    "build-tsc": "tsc",
    "clean": "rm -rf dist && rm -rf .next",
    "copy-env": "cp .env .env.production dist",
    "dev": "bash -c 'NODE_CONFIG_ENV=dev next dev'",
    "lint": "next lint",
    "next-start": "bash -c 'NODE_CONFIG_ENV=dev next start'",
    "restart": "npm run build && npm run start",
    "start": "node dist/src/index.js",
    "test": "bash -c 'NODE_CONFIG_ENV=test jest --coverage'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorekpo/music-app.git"
  },
  "keywords": [
    "music",
    "music",
    "library",
    "DJ",
    "Algorithm",
    "algorithm.v"
  ],
  "author": "Victor E.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victorekpo/music-app/issues"
  },
  "homepage": "https://github.com/victorekpo/music-app#readme",
  "dependencies": {
    "@apollo/client": "^3.10.0-rc.1",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
    "@apollo/server": "^4.10.4",
    "@as-integrations/next": "^3.0.0",
    "bunyan": "^1.8.15",
    "config": "^3.3.11",
    "csv-parser": "^3.0.0",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "googleapis": "^140.0.0",
    "graphql": "^16.9.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.2",
    "papaparse": "^5.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-syntax-highlighter": "^15.5.0",
    "readline": "^1.3.0",
    "yargs": "^17.1.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/config": "^3.3.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "nodemon": "^3.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5"
  }
}
